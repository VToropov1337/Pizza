<h2>Welcome to pizza-shop!</h2>

	<table class="table table-striped"> 
		<thead> 
			<tr>
			 	<th>Name</th> 
			 		<th>Price</th> 
			 			<th>Qty</th> 
			</tr> 
		</thead>
				<tbody> 
						<% @items.each do |item| %>
					<tr> 
							<td><%= item[0].title %></td> 
							<td><%= item[0].price%></td> 
							<td><%= item[1] %></td> 
					</tr> 
						<% end %>
					<tr> 
			</tbody> 
	</table> 